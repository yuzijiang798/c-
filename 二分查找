//二分查找
#include "iostream"
using namespace std;
#include "algorithm"
#include "vector"
void test(){
    vector<int>v;
    for(int i=0;i<10;i++){
        v.push_back(i);
    }
   bool ret=binary_search(v.begin(),v.end(),2);
    if(ret){
        cout<<"z找到了"<<endl;
    } else
    {
        cout<<"w未找到"<<endl;
    }

}
int main(){
    test();
    return 0;
}
//二分查找容器中的元素  必须是有序序列
