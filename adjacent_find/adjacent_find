#include "iostream"
using namespace std;
#include "algorithm"
#include "vector"
void test(){
    vector<int>v;
    v.push_back(0);
    v.push_back(2);
    v.push_back(0);
    v.push_back(4);
    v.push_back(66);
    v.push_back(0334);
    v.push_back(30);
  vector<int>::iterator it= adjacent_find(v.begin(),v.end());
    if (it== v.end()){
        cout<<"未找到相邻重复"<<endl;
    } else
    {
        cout<<"找到了相邻重复元素"<<endl;
    }
}
int main(){
    test();
    return 0;
}
